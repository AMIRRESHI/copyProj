{
  "name": "angular-io-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "ng serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~6.0.4",
    "@angular/common": "~6.0.4",
    "@angular/compiler": "~6.0.4",
    "@angular/compiler-cli": "~6.0.4",
    "@angular/core": "~6.0.4",
    "@angular/forms": "~6.0.4",
    "@angular/http": "~6.0.4",
    "@angular/platform-browser": "~6.0.4",
    "@angular/platform-browser-dynamic": "~6.0.4",
    "@angular/platform-server": "~6.0.4",
    "@angular/router": "~6.0.4",
    "@angular/upgrade": "~6.0.4",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "angular-in-memory-web-api": "~0.6.0",
    "core-js": "^2.5.7",
    "ngrx-data": "^6.0.2-beta.10",
    "ngrx-store-freeze": "^0.2.4",
    "rxjs": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@ngrx/schematics": "^6.0.1",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.2",
    "codelyzer": "~4.2.1",
    "jasmine-marbles": "^0.3.1",
    "jest": "^23.1.0",
    "jest-preset-angular": "^5.2.2",
    "lodash": "^4.17.10",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "~5.3.2",
    "rxjs-compat": "^6.2.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "2.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "coveragePathIgnorePatterns": [
      "/jest-config/",
      "/node_modules/"
    ],
    "setupTestFrameworkScriptFile": "./jest-config/setup.ts",
    "moduleNameMapper": {
      "@appContainers/(.*)": "<rootDir>/src/app/containers/$1",
      "@appComponents/(.*)": "<rootDir>/src/app/components/$1",
      "@appServices/(.*)": "<rootDir>/src/app/services/$1",
      "@appStore/(.*)": "<rootDir>/src/app/store/$1",
      "@appGuards/(.*)": "<rootDir>/src/app/guards/$1",
      "@appModels/(.*)": "<rootDir>/src/app/models/$1"
    }
  },
  "repository": {}
}
